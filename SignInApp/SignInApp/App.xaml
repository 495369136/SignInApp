<Application x:Class="SignInApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SignInApp"
             StartupUri="LoginWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/SignInApp;component/Styles/TextButton.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/SignInApp;component/Styles/ImageButton.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/SignInApp;component/Styles/TableItem.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/SignInApp;component/Styles/RadioButton.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/SignInApp;component/Styles/RadioTextButton.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/SignInApp;component/Styles/CheckBox.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/SignInApp;component/Styles/ProgressBar.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/SignInApp;component/Styles/Menu.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
